        <!-- GmosNFpuIfu => GmosNorthType.FPUnitNorth -->
        <jxb:bindings node="./xsd:simpleType[@name='GmosNFpuIfu']/xsd:restriction">
            <jxb:bindings node="./xsd:enumeration[@value='IFU 2 slits']">
                <jxb:typesafeEnumMember name="IFU_1"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='IFU 1 slit']">
                <jxb:typesafeEnumMember name="IFU_3"/>
            </jxb:bindings>
        </jxb:bindings>

    </jxb:bindings>
