            <jxb:bindings node="./xsd:enumeration[@value='z (925 nm)']">
                <jxb:typesafeEnumMember name="z_G0304"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='Z (876 nm)']">
                <jxb:typesafeEnumMember name="Z_G0322"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='Y (1010 nm)']">
                <jxb:typesafeEnumMember name="Y_G0323"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='HeII (468 nm)']">
                <jxb:typesafeEnumMember name="HeII_G0320"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='HeIIC (478 nm)']">
                <jxb:typesafeEnumMember name="HeIIC_G0321"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='OIII (499 nm)']">
                <jxb:typesafeEnumMember name="OIII_G0318"/>
