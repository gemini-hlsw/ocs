                <jxb:typesafeEnumMember name="LONGSLIT_7"/>
            </jxb:bindings>
        </jxb:bindings>

        <!-- GmosNMOSFpu => GmosNorthType.FPUnitNorth -->
        <jxb:bindings node="./xsd:simpleType[@name='GmosNMOSFpu']/xsd:restriction">
            <jxb:bindings node="./xsd:enumeration[@value='0.5 arcsec slit']">
                <jxb:typesafeEnumMember name="LONGSLIT_2"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='0.75 arcsec slit']">
