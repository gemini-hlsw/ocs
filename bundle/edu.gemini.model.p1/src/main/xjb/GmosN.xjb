<jxb:bindings version="2.0"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <!-- Custom bindings for GmosN -->
    <jxb:bindings schemaLocation="../xsd/GmosN.xsd" node="/xsd:schema">

        <!-- GmosNDisperser => GmosNorthType.DisperserNorth -->
        <jxb:bindings node="./xsd:simpleType[@name='GmosNDisperser']/xsd:restriction">
            <jxb:bindings node="./xsd:enumeration[@value='B1200']">
                <jxb:typesafeEnumMember name="B1200_G5301"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='R831']">
                <jxb:typesafeEnumMember name="R831_G5302"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='B600']">
                <jxb:typesafeEnumMember name="B600_G5307"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='R600']">
                <jxb:typesafeEnumMember name="R600_G5304"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='R400']">
                <jxb:typesafeEnumMember name="R400_G5305"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='R150']">
                <jxb:typesafeEnumMember name="R150_G5308"/>
            </jxb:bindings>
        </jxb:bindings>

        <!-- GmosNFilter => GmosNorthType.FilterNorth -->
        <jxb:bindings node="./xsd:simpleType[@name='GmosNFilter']/xsd:restriction">
            <jxb:bindings node="./xsd:enumeration[@value='GG455 (&gt; 460 nm)']">
                <jxb:typesafeEnumMember name="GG455_G0305"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='OG515 (&gt; 520 nm)']">
                <jxb:typesafeEnumMember name="OG515_G0306"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='RG610 (&gt; 615 nm)']">
                <jxb:typesafeEnumMember name="RG610_G0307"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='g + GG455 (506 nm)']">
                <jxb:typesafeEnumMember name="g_G0301_GG455_G0305"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='g + OG515 (536 nm)']">
                <jxb:typesafeEnumMember name="g_G0301_OG515_G0306"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='r + RG610 (657 nm)']">
                <jxb:typesafeEnumMember name="r_G0303_RG610_G0307"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='i + CaT (815 nm)']">
                <jxb:typesafeEnumMember name="i_G0302_CaT_G0309"/>
