            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='1.0 arcsec slit']">
                <jxb:typesafeEnumMember name="LONGSLIT_4"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='1.5 arcsec slit']">
                <jxb:typesafeEnumMember name="LONGSLIT_5"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='2.0 arcsec slit']">
                <jxb:typesafeEnumMember name="LONGSLIT_6"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='5.0 arcsec slit']">
                <jxb:typesafeEnumMember name="LONGSLIT_7"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='Other']">
                <jxb:typesafeEnumMember name="CUSTOM_MASK"/>
            </jxb:bindings>
        </jxb:bindings>

        <!-- GmosNFpuNs => GmosNorthType.FPUnitNorth -->
        <jxb:bindings node="./xsd:simpleType[@name='GmosNFpuNs']/xsd:restriction">
            <jxb:bindings node="./xsd:enumeration[@value='0.25 arcsec slit']">
                <jxb:typesafeEnumMember name="NS_O"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='0.5 arcsec slit']">
                <jxb:typesafeEnumMember name="NS_1"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='0.75 arcsec slit']">
                <jxb:typesafeEnumMember name="NS_2"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='1.0 arcsec slit']">
                <jxb:typesafeEnumMember name="NS_3"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='1.5 arcsec slit']">
                <jxb:typesafeEnumMember name="NS_4"/>
            </jxb:bindings>
            <jxb:bindings node="./xsd:enumeration[@value='2.0 arcsec slit']">
                <jxb:typesafeEnumMember name="NS_5"/>
